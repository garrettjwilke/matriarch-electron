{
  "name": "matriarcheditor",
  "productName": "matriarch editor",
  "version": "1.0.0",
  "description": "matriarch editor",
  "main": "main.js",
  "scripts": {
    "start": "electron . --ozone-platform=wayland",
    "package": "electron-builder"
  },
  "build": {
    "appId": "com.moog.matriarcheditor",
    "linux": {
      "category": "tool",
      "executableName": "matriarcheditor",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "win": {
      "target": [
        "zip"
      ]
    }
  },
  "keywords": [
    "moog",
    "matriarch",
    "editor"
  ],
  "author": "homeless",
  "devDependencies": {
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3"
  }
}
